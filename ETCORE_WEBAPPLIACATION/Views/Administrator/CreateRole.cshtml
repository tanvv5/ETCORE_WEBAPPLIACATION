@model CreateRoleViewModel
@{
    ViewBag.Title = "Create Role";
}
<form enctype="multipart/form-data" asp-controller="Administrator" asp-action="CreateRole" method="post" class="mt-3">
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group row">
        <label asp-for="RoleName" class="col-form-label col-sm-2">RoleName</label>
        <div class="col-sm-10">
            <input asp-for="RoleName" class="form-control" aria-describedby="emailHelp">
            <span asp-validation-for="RoleName"></span>
        </div>
    </div>
    <div style="margin-top:15px">
        <output name="result"></output>
    </div>
    <button type="submit" class="btn btn-primary">Create Role</button>
</form>